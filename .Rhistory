N <- 1000 #number of samples
n <- 10   #number of observations per sample
T_N <- rep(0, N) #placeholder (to store sample means)
for(i in 1:N){
this_sample <- runif(n)
this_sample_mean <- mean(this_sample)
T_N[i] <- this_sample_mean
}
mean(T_N)
